		<add key="provincePredictionTable" value="T_AIR_PREDICT_PROVINCE" />
		<!--国家城市预报表-->
		<add key="cityPredictionTable" value="T_AIR_PREDICT_CITY" />

		<!--气象实时服务地址-->
