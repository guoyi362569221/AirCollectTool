	<appSettings>
		<!--数据库文件名称-->
		<add key="configDataBaseName" value="monitorDataBase.sqlite" />
		<!--国家空气检测总站上的silverlight文件的绝对地址-->
		<add key="dataUrl" value="http://106.37.208.233:20035/ClientBin/cnemc.xap" />
		<!--小时城市-->
		<add key="hourCity" value="T_AIR_CITY_HOUR_QUALITY" />
		<!--小时站点-->
		<add key="hourStation" value="T_AIR_SITE_HOUR_QUALITY" />
		<!--日均城市-->
		<add key="dayCity" value="T_AIR_CITY_DAY_QUALITY" />
		<!--日均站点-->
		<add key="dayStation" value="T_AIR_SITE_DAY_QUALITY" />
		<!--无效数据值-->
		<add key="noDataValue" value="-1" />
		<!--aqi等级格式，int 代表 转换成数字，其他为汉字-->
		<add key="aqiLevelFormat" value="int" />
		
		<!--国家区域预报服务地址-->
		<add key="areaPredictionUrl" value="http://106.37.208.228:8082/AreaForecast/ChangeArea" />
		<!--国家省域预报服务地址-->
		<add key="provincePredictionUrl" value="http://106.37.208.228:8082/ProvinceForecast/GetGlobalInfo" />
		<!--国家城市预报服务地址-->
		<add key="cityPredictionUrl" value="http://106.37.208.228:8082/Home/Default" />

